<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eif.viko.lt.multiscreenjavafx.ProfilePreviewController">
   <children>
      <Label fx:id="profileName" layoutX="64.0" layoutY="29.0" text="profileName" textFill="#179cff">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Button layoutX="64.0" layoutY="85.0" mnemonicParsing="false" onAction="#closeProfilePreview" text="Close profile preview" textFill="#a8997f">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
